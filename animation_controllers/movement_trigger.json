{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.alien.movement": {
			"initial_state": "idle",
			"states": {
				"moving": {
					"transitions": [
						{
							"idle": "!query.is_moving"
						}
					],
					"animations": [
						"movement_score"
					]
				},
				"idle": {
					"transitions": [
						{
							"moving": "query.is_moving"
						}
					]
				}
			}
		}
	}
}