{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.alien.afk": {
			"states": {
				"moving": {
					"transitions": [
						{
							"afk": "!q.is_moving && q.is_alive"
						}
					],
					"on_entry": [
						"/scoreboard players reset @s afk_check"
					]
				},
				"afk": {
					"animations": [
						"alien_afkCheck.a",
						"alien_afkTime.a"
					],
					"transitions": [
						{
							"moving": "q.is_moving"
						}
					],
					"on_exit": [
						"/scoreboard players reset @s afk_check"
					]
				}
			}
		}
	}
}