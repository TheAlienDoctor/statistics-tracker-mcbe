{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.alien.time": {
            "initial_state": "default",
            "states": {
                "default": {
                    "transitions": [
                        {
                            "not_playing": "!q.is_alive"
                        }
                    ],
					"animations": [
						"alien_time.a"
					]
                },
                "not_playing": {
                    "transitions": [
                        {
                            "default": "q.is_alive"
                        }
                    ]
                }
            }
        }
    }
}